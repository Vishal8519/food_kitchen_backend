{
  "functions": {
    "api": {
      "runtime": "python3.8", 
      "handler": "food_kitchen/wsgi.handler" 
    }
  },
  "builds": [
    {
      "src": "food_kitchen/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "food_kitchen/wsgi.py"
    }
  ]
}